pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building Hello World App'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying Hello World App'
            }
        }
        stage('Test') {
            steps {
                echo 'Test Hello World App'
            }
        }
        stage('Release') {
            steps {
                echo 'Release Hello World App'
            }
        }
    }
}
